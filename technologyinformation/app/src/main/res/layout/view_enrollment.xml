<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/lin_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="110dip"
            android:text="我要报名"
            android:textColor="@color/color9"
            android:textSize="@dimen/text_18sp"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/lin_em_name"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_marginLeft="26dip"
            android:layout_marginRight="26dip"
            android:layout_marginTop="20dip"
            android:background="@drawable/bg_shape_enrollment"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="20dip"
            android:paddingRight="20dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv106"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <TextView
                android:id="@+id/tv_em_name"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:layout_weight="1"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_em_address"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_marginLeft="26dip"
            android:layout_marginRight="26dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/bg_shape_enrollment"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="20dip"
            android:paddingRight="20dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv107"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <TextView
                android:id="@+id/tv_em_address"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:layout_weight="1"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/enrollment_zs" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_em_profession"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_marginLeft="26dip"
            android:layout_marginRight="26dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/bg_shape_enrollment"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="20dip"
            android:paddingRight="20dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv108"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <TextView
                android:id="@+id/tv_em_profession"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:layout_weight="1"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/enrollment_zs" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_em_program"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_marginLeft="26dip"
            android:layout_marginRight="26dip"
            android:layout_marginTop="10dip"
            android:background="@drawable/bg_shape_enrollment"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="20dip"
            android:paddingRight="20dip">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tv109"
                android:textColor="@color/color29"
                android:textSize="@dimen/text_16sp" />

            <EditText
                android:id="@+id/et_em_program"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:background="@null"
                android:lines="1"
                android:textColor="@color/color29"
                android:textColorHint="@color/textProfileGray"
                android:textSize="@dimen/text_16sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/tv_dialog_confirm"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_below="@+id/lin_sign_addday"
            android:layout_marginBottom="20dip"
            android:layout_marginLeft="26dip"
            android:layout_marginRight="26dip"
            android:layout_marginTop="10dip"
            android:gravity="center"
            android:text="@string/tv27"
            android:textColor="@color/white"
            android:textSize="@dimen/text_16sp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dip"
        android:src="@mipmap/enrollment_8" />
</LinearLayout>